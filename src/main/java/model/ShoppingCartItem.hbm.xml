<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="model.ShoppingCartItem" table="shoppingcartitems">
        <composite-id>
            <key-property name="user_id" column="user_id" type="int"></key-property>
            <key-property name="book_id" column="book_id" type="long"></key-property>
        </composite-id>
        <property name="number" column="number" type="int"></property>
	</class>
</hibernate-mapping>