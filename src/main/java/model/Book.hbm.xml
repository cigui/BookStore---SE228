<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="model.Book" table="books">
		<id name="id" type="long">
			<column name="id" />
			<generator class="assigned" />
		</id>
		<property name="book_name" type="java.lang.String">
			<column name="book_name" />
		</property>
		<property name="book_author" type="java.lang.String">
			<column name="book_author" />
		</property>
		<property name="book_price" type="int">
			<column name="book_price" />
		</property>
		<property name="book_publisher" type="java.lang.String">
			<column name="book_publisher" />
		</property>
		<property name="book_category" type="java.lang.String">
			<column name="book_category" />
		</property>
		<property name="inventory" type="int">
			<column name="inventory" />
		</property>
	</class>
</hibernate-mapping>